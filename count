b, c, d, k = [], [], 0, 0
for i in a:
    c = 0
    for j in i:
        c += int(j)
    b.append(c / 3)
print(*b, sep='\n')
for i in range(len(a)):
    d += int(a[i][0])
k.append(d / 3)
d = 0
for i in range(len(a)):
    d += int(a[i][1])
k.append(d / 3)
d = 0
for i in range(len(a)):
    d += int(a[i][2])
k.append(d / 3)
d = 0
print(*k)
