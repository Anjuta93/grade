a = [[85, 92, 78], [100, 88, 94], [58, 72, 85]]
b = []
k = []
d = 0

for i in a:
    c = 0
    for j in i:
        c += j
    b.append(c / 3)
print(*b, sep='\n')

for i in range(len(a)):
    d += a[i][0]
k.append(d / 3)
d = 0
for i in range(len(a)):
    d += a[i][1]
k.append(d / 3)
d = 0
for i in range(len(a)):
    d += a[i][2]
k.append(d / 3)
d = 0
print(*k)
